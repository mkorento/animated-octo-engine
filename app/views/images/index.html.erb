<h1>Photo app!</h1>

<%= link_to 'Upload image', new_image_path %>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |i| %>
      <tr>
        <td><%= link_to(image_tag(i.file_url, size: '75'), i) %><td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
